{
  "name": "snowball-pwa-static-template",
  "short_name": "snowball",
  "description": "snowball tools static template app",
  "theme_color": "#3390ff",
  "background_color": "#ffffff",
  "display": "standalone",
  "orientation": "portrait",
  "scope": "/",
  "start_url": "/",
  "display_override": ["window-controls-overlay"],
  "lang": "en-US",
  "dir": "auto",
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/email?address=%s"
    },
    {
      "protocol": "webcal",
      "url": "/calendar?address=%s"
    }
  ],
  "icons": [
    {
      "src": "assets/icons/icon_512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "assets/icons/icon_192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "screenshots": [
    {
      "src": "assets/screenshots/screenshot.png",
      "sizes": "1280x720",
      "type": "image/png"
    }
  ],
  "features": ["Cross Platform", "fast", "simple"],
  "categories": [],
  "shortcuts": [
    {
      "name": "Snowball Tools",
      "short_name": "Snowball Tools",
      "description": "Snowball Tools",
      "url": "/",
      "icons": [
        {
          "src": "assets/icons/icon_512.png",
          "sizes": "512x512",
          "type": "image/png",
          "purpose": "maskable any"
        }
      ]
    }
  ]
}
